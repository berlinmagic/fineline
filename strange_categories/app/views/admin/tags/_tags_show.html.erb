<% data ||= @data %>
<div class="box">
	<div class="in_box" id="data_tags_box">
		<% if data.tags.count > 0 %>
			<%= render 'admin/tags/objekt_tags', :data => data %>
		<% end %>
	</div>
</div>

<div class="box">
	<div class="trenn_box">
		<div class="top_trenner"></div>
	</div>
</div>

<% if current_user %>

<div class="box">
	<div class="in_box">
		<h2 class="polymorphe_form_head" id="tag_get_available">
			<%= link_to 'Tag´s hinzufügen', "/admin/#{data.class.to_s.tableize}/#{data.id.to_s}/tags/available_data_tags", :remote => true %>
		</h2>
		<div id="taggs_js_magick_place"></div>
		<div class="clearfix"></div>
		<br/>
	</div>
</div>

<% end %>