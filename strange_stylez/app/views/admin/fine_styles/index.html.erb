<%= render 'admin/shared/stylez_sub_menu' %>

<div class="admin_box">
	<h1 class="icon"><%= finelineIcon(:icon => 71, :color => finestyler('headline_icon_color'), :size => finestyler('headline_icon_size'), :blur => finestyler('headline_icon_blur')) %><%= t('strange_stylez.headlines.index') %></h1>

	<div class="admin_in_box">
		<div class="system_help_text<%= ' hidden' unless Strangecms::Config[:show_help] %>">
			<div class="box">
				<div class="in_box">
					<p class="sys_help"><%= t('strange_stylez.helptext.index') %></p>
				</div>
			</div>
		</div>
		
		<div class="box">
				<div class="trenn_linie"></div>
		</div>
		

		<div class="work_box">
			<div class="in_box white">
				<ul class="toggle_list">
					<% @fine_stylez.each do |style| %>
						
						<li>
							<%= link_to style.name, admin_fine_style_path(style) %> .. [ <%= style.theme_name %> ]
							<div>..</div>
						</li>
						
					<% end %>
				</ul>
			</div>
		</div>
		<div class="box">
				<div class="trenn_linie"></div>
		</div>
		

		
	</div>
</div>

<%= render 'sidebar' %>


