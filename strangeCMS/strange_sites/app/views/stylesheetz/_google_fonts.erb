<% google_one = false %>
<% google_two = false %>
<% google_three = false %>

<% google_one = 	true if Strangecms::Stylez::Config[:front_body_font_family] == 'Google_one' %>
<% google_two = 	true if Strangecms::Stylez::Config[:front_body_font_family] == 'Google_two' %>
<% google_three = 	true if Strangecms::Stylez::Config[:front_body_font_family] == 'Google_three' %>



<% if google_one %>
	<link href='http://fonts.googleapis.com/css?family=<%= Strangecms::Stylez::Config[:google_font_one] %>' rel='stylesheet' type='text/css'>
<% end %>
<% if google_two %>
	<link href='http://fonts.googleapis.com/css?family=<%= Strangecms::Stylez::Config[:google_font_two] %>' rel='stylesheet' type='text/css'>
<% end %>
<% if google_three %>
	<link href='http://fonts.googleapis.com/css?family=<%= Strangecms::Stylez::Config[:google_font_three] %>' rel='stylesheet' type='text/css'>
<% end %>
