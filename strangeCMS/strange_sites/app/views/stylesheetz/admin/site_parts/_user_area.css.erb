#fineAdminExit, #fineAdminEnter { padding: 0 20px 0 10px; margin: 10px 0; font-size:13px; font-weight: normal; display:block; }
#fineAdminExit:hover { border: solid 1px #369; }
#fineAdminExit .finelineIcon, #fineAdminEnter .finelineIcon { margin: 1px 10px 1px 0; }

#strange_admin_button { padding: 0 10px; border: solid 1px #ccc; margin: 10px 0; background: #fff; line-height: 20px;
		<%= fine_borderRadius( '2px' ) %>  color: #444; display:block; float:left;} 
#strange_admin_button.admin_area { background: #336699; }
#strange_admin_button:hover { background: #de9f42 url('/images/strange_admin_button_bg_o.png') repeat-x; color: #000; }


#login_area { margin: 11px 5px; color: #f3f3f3; line-height: 20px; display:inline-block; }

#finelineLogout { padding: 0 10px; background: #000; line-height: 20px; <%= fine_borderRadius( '3px' ) %>  color: #999;  display:inline-block; margin:10px 0; }
#finelineLogout:hover { color: #fff;}
.right_user_nav { float: right; }