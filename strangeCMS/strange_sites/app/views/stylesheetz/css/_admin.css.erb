header { height: 44px; background: #fff url('/images/admin/head_nav_bg.jpg') left top repeat-x; border-bottom: solid 1px #c6c6c6;}
#main_navigation { height: 34px; position:absolute; top: 10px; left: 0px; display:block; }
#main_navigation a { line-height: 18px; padding: 6px 10px 5px; border: solid 1px transparent; border-bottom: solid 1px #c6c6c6; margin: 5px 3px 0 0; position:relative; display:block; float:left; background: transparent url('/images/strange_admin_navigation_bg.jpg') left top repeat;
-webkit-border-top-left-radius: 2px; -webkit-border-top-right-radius: 2px; -moz-border-radius-topleft: 2px; border: 0;
	-moz-border-radius-topright: 2px; border-top-left-radius: 2px; border-top-right-radius: 2px; color: #fff; font-size: 15px;}
#main_navigation a:hover, #main_navigation a.aktiv { padding: 7px 10px 5px; margin-top: 3px; background: #fff url('/images/admin/head_nav_aktiv.jpg') left top repeat-x; color:#336699; border: solid 1px #bdbcbd; border-bottom: solid 1px transparent;}
#main_navigation a.aktiv:hover { color: #444; }


#main_right_navigation { height: 34px; position:absolute; top: 10px; right: 0px; display:block; }
#main_right_navigation a { line-height: 18px; padding: 6px 10px 5px; border: solid 1px transparent; border-bottom: solid 1px #c6c6c6; margin: 5px 3px 0 0; position:relative; display:block; float:left; background: transparent url('/images/strange_admin_navigation_bg.jpg') left top repeat;
-webkit-border-top-left-radius: 2px; -webkit-border-top-right-radius: 2px; -moz-border-radius-topleft: 2px; border: 0;
	-moz-border-radius-topright: 2px; border-top-left-radius: 2px; border-top-right-radius: 2px; color: #fff; font-size: 15px;}
#main_right_navigation a:hover, #main_right_navigation a.aktiv { padding: 7px 10px 5px; margin-top: 3px; background: #fff url('/images/admin/head_nav_aktiv.jpg') left top repeat-x; color:#336699; border: solid 1px #bdbcbd; border-bottom: solid 1px transparent;}
#main_right_navigation a.aktiv:hover { color: #444; }

#sub-menu #in_sub { height: 33px; background: #fff url('/images/admin/head_sub_nav_bg.jpg') left top repeat-x; border-bottom: solid 1px #c6c6c6;}

#sub-menu #in_sub .kasten {height: 33px; }

#sub-menu #in_sub .kasten #sub_navigation { height: 27px; position: absolute; top: 2px; left: 0; }
#sub-menu #in_sub .kasten #sub_navigation a { line-height: 18px; padding: 4px 10px 3px; border: solid 1px transparent; border-bottom:0; margin: 5px 3px 0 0; position:relative; display:block; float:left; background: transparent;
-webkit-border-top-left-radius: 2px; -webkit-border-top-right-radius: 2px; -moz-border-radius-topleft: 2px; -moz-border-radius-topright: 2px; border-top-left-radius: 2px; border-top-right-radius: 2px; color: #666; font-size: 15px;}
#sub-menu #in_sub .kasten #sub_navigation a:hover, #sub-menu #in_sub .kasten #sub_navigation a.aktiv { background: #fff; border: 1px solid #c6c6c6; border-bottom: solid 1px #fff; color: #336699;}
#sub-menu #in_sub .kasten #sub_navigation a.aktiv:hover{ color: #444;}

