/* tooltip styling. by default the element to be styled is .tooltip  */
.tooltip {
	display:none;
	background:transparent url(/images/tips/tip_k.png);
	font-size:18px; font-weight: bold;
	line-height: 20px;
	text-align: left;
	height:31px;
	width: 170px;
	padding:25px 15px;
	color:#725311;	
	text-shadow: 0px 0px 3px #fff;
}
.tooltip.del_link { color:#000; }
.tooltip.del_link span { width: 32px; height:32px; margin: -5px 12px 0 10px; background: url('/images/btns/delete_b.png'); float: left;}

.tooltip.edit_link span { width: 32px; height:32px; margin: -5px 10px 0 8px; background: url('/images/btns/edit_b.png'); float: left;}


