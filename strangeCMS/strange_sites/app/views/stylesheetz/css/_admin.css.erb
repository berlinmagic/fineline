table.mb_table {width: 95%; border-spacing: 0; border: 0; position: relative; }
table.mb_table th { padding: 10px 5px; background: rgba(230,230,230,.7); position: relative; }
table.mb_table td { padding: 10px 5px; position: relative; }

table.mb_table th.title, table.mb_table td.title { width: 58%; }
table.mb_table th.sorter, table.mb_table td.sorter { width: 7%; padding: 2px 5px;}
table.mb_table th.aktiv, table.mb_table td.aktiv { width: 5%;}
table.mb_table th.actions, table.mb_table td.actions { width:30%;}
table.mb_table tr { position: relative;  }

table.mb_table tr td.actions { text-align:right;}
table.mb_table tr td.sorter span { visibility:hidden;}
table.mb_table tr td.actions a { visibility:hidden; }

table.mb_table tr:hover { background: rgba(5,230,30,.2); }
table.mb_table tr:hover td.actions a { visibility:visible; }
table.mb_table tr:hover td.sorter span { visibility:visible;}

table.settings.kontakt tr { border-top: solid 1px #999;}
table.settings.kontakt tr td { padding: 10px 0 10px 30px;}
table.settings.kontakt tr th { font-size: 15px; color: #666; width: 175px; font-weight: normal;}
table.settings.kontakt tr.first { border: 0;}

table.mb_table td.sorter span.handler { cursor:pointer; width: 22px; background: transparent url('/images/schieber_k.png'); height: 22px; position:relative; display:block; left:0; top:5px; vertical-align: center;}

table.settings th.u_kat { color: #66cc00; padding: 10px 0 0;}


div.mb_table {width: 100%; border-spacing: 0; border: 0; position: relative; display: table; line-height: 29px; }

div.mb_table .row { position: relative; width: 100%; display: table-row; }

div.mb_table .title { width: 60%; padding: 10px 5px; display: table-cell; }
div.mb_table .sorter { width: 5%; padding: 2px 5px; display: table-cell; }
div.mb_table .aktiv { width: 5%; padding: 10px 5px; display: table-cell; }
div.mb_table .actions { width:30%; padding: 10px 5px; display: table-cell; }


div.mb_table .row .actions { text-align:right;}
div.mb_table .row .sorter span { visibility:hidden;}
div.mb_table .row .actions a { visibility:hidden; }

div.mb_table .row:hover { background: rgba(5,230,30,.2); }
div.mb_table .row:hover .actions a { visibility:visible; }
div.mb_table .row:hover .sorter span { visibility:visible;}

div.mb_table .sorter span.handler { cursor:pointer; width: 22px; background: transparent url('/images/schieber_k.png'); height: 22px; position:relative; display:block; left:0; top:5px; vertical-align: center;}

div.mb_table .row .unter_seiten {width: 90%; border-spacing: 0; border: 0; position: relative; display: table; }
div.mb_table .row .unter_seiten .u_row { position: relative; width: 100%; display: table-row; }
div.mb_table .row .unter_seiten .u_row > * {display: table-cell; vertical-align: top;}

#xabc .section-group       {display: table; width: 100%;}
#xabc .section-group-inner {display: table-row;}
#xabc .section-group-inner > * {display: table-cell; vertical-align: top;}



